{
  "expo": {
    "name": "testing_app_frontend",
    "slug": "testing_app_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets_dope/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets_dope/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets_dope/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets_dope/favicon.png"
    }
  }
}
